{
    "name": "AustinMaddox/s3",
    "type": "phpbb-extension",
    "description": "An extension that utilizes AWS S3 as the filesystem for uploaded files.",
    "homepage": "https://github.com/AustinMaddox/phpbb-extension-s3",
    "version": "1.0.4",
    "time": "2019-12-31",
    "keywords": [
        "phpbb",
        "extension",
        "amazon web service",
        "AWS",
        "simple storage service",
        "s3",
        "phpbb-extension-s3"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Austin Maddox",
            "email": "austin@maddoxbox.com",
            "homepage": "https://github.com/AustinMaddox/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "aws/aws-sdk-php": "^3.130",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "AWS S3",
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/aws_s3",
            "filename": "version_check"
        },
        "soft-require": {
			"phpbb/phpbb": "~3.2.0"
		}
    }
}
